    <section class="">
        <div id="carouselExampleSlidesOnly" class="carousel slide" data-bs-interval="2000" data-bs-ride="carousel">
            <div class="carousel-inner">
                {{#each banners}}
                <div class="carousel-item active">
                    <img src="/images/product/{{this.img.[0]}}" class="d-block w-100" alt="...">
                </div>
                {{/each}}


            </div>
        </div>
    </section>

    <!-- Banner Section Begin -->
    <section class="banner spad">
        <div class="container">
            <div class="row">
                <div class="col-lg-7 offset-lg-5">
                    <div class="banner__item">
                        <div class="banner__item__pic">
                            <img src={{categoryDetails.[0].img}} style="height:24rem;width:27.5rem"alt="">
                        </div>
                        <div class="banner__item__text">
                            <h2><span>{{categoryDetails.[0].category}}</span>  Collections 2022</h2>
                            <a href="/category-wise-shop-now/{{categoryDetails.[0]._id}}">Shop now</a>
                        </div>
                    </div>
                </div>
                <div class="col-lg-5">
                    <div class="banner__item banner__item--middle">
                        <div class="banner__item__pic">
                            <img src={{categoryDetails.[1].img}} alt="">
                        </div>
                        <div class="banner__item__text">
                            <h2><span>{{categoryDetails.[1].category}}</span>  Collections</h2>
                            <a href="/category-wise-shop-now/{{categoryDetails.[1]._id}}">Shop now</a>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </section>
    <!-- Banner Section End -->

    <!-- Product Section Begin -->
     <section class="product spad mb-5">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <ul class="filter__controls">
                        <li class="active" data-filter="*">Best Sellers</li>
                        
                    </ul>
                </div>
            </div>
            <div class="row product_filter">
               
             {{#each topSellingProducts}}
                 <div class="col-lg-3 col-md-6 col-sm-6 col-md-6 col-sm-6 mix hot-sales">
                     <div class="product__item">
                                <div class="product__item__pic set-bg" data-setbg="">
                                    <a href="/product-details/{{this.productInfo._id}}" ><img src="/images/product/{{this.productInfo.img.[0]}}"+ alt="" style="height: 20rem; width:20rem"></a>
                                    {{#if this.comparedDiscount}}
                                    <span class="label">{{this.comparedDiscount}}%</span>
                                    {{/if}}
    
                                </div>
                                <div class="product__item__text">
                                    <h6 class="text-center" style="margin-top: 5rem;">{{this.name}}</h6>
                                    {{#if this.productInfo.quantity}}
                                    <button class="add-cart border-0 btn btn-transparent fw-bold"  style="margin-left: 5.5rem;color:brown;" onclick="addToCart('{{this._id}}')">Add to Cart</button>
                                    {{else}}
                                    <p class="text-danger text-center">Out of stock!!</p>
                                    {{/if}}
                                   {{#if this.comparedDiscount}}
                                    <div class="d-flex justify-content-around">
                                        <span class="text-muted text-decoration-line-through">{{this.productInfo.price}}</span>
                                    <strong><span class="text-center">₹ {{this.discountedPrice}}</span></strong>
                                    <span class="text-success"> {{this.comparedDiscount}}%</span>
                                    </div>
                                    {{else}}
                                    <div class="text-center">
                                        <strong><span class="">₹ {{this.productInfo.price}}</span></strong>
                                     </div>
                                    {{/if}}
                                </div>
                            </div>
                </div>
                {{/each}}
               

             </div> 
        </div> 

        
     </section> 
    <!-- Product Section End -->

